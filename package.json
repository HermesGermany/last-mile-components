{
  "name": "@hermesgermany/last-mile-components",
  "version": "0.0.6",
  "type": "module",
  "main": "./dist/lmc.umd.js",
  "module": "./dist/lmc.es.js",
  "types": "./dist/lmc.es.d.ts",
  "scripts": {
    "dev": "npm run storybook",
    "build": "tsc && vite build",
    "storybook": "storybook dev -h localhost -p 6006",
    "build-storybook": "storybook build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ci": "vitest run",
    "generate": "plop",
    "lint": "eslint \"src/**/*.ts\" \"src/**/*.tsx\"",
    "format": "prettier --write"
  },
  "exports": {
    ".": {
      "import": "./dist/lmc.es.js",
      "require": "./dist/lmc.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@headlessui/react": "1.7.13",
    "clsx": "1.2.1",
    "react-popper": "2.3.0",
    "tailwindcss": "3.3.1"
  },
  "peerDependencies": {
    "react": ">17",
    "react-dom": ">17"
  },
  "devDependencies": {
    "@babel/core": "7.21.4",
    "@storybook/addon-actions": "7.0.4",
    "@storybook/addon-docs": "7.0.8",
    "@storybook/addon-essentials": "7.0.4",
    "@storybook/addon-interactions": "7.0.4",
    "@storybook/addon-links": "7.0.4",
    "@storybook/addon-styling": "1.0.0",
    "@storybook/react": "7.0.4",
    "@storybook/react-vite": "7.0.4",
    "@storybook/testing-library": "0.1.0",
    "@testing-library/react": "14.0.0",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "5.57.0",
    "@vitejs/plugin-react": "3.1.0",
    "autoprefixer": "10.4.14",
    "babel-loader": "8.3.0",
    "eslint": "8.37.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "jsdom": "21.1.1",
    "plop": "3.1.2",
    "postcss": "8.4.21",
    "prettier": "2.8.7",
    "prettier-plugin-organize-imports": "3.2.2",
    "prettier-plugin-tailwindcss": "0.2.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "storybook": "7.0.4",
    "typescript": "4.9.3",
    "vite": "4.2.0",
    "vite-plugin-dts": "2.2.0",
    "vitest": "0.29.8"
  },
  "description": "This is a library of reusable React components for building web apps for the last-mile.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HermesGermany/last-mile-components.git"
  },
  "author": "@hermesgermany",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/HermesGermany/last-mile-components/issues"
  },
  "homepage": "https://github.com/HermesGermany/last-mile-components#readme"
}